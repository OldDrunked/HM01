<HTML>

<head/>

<BODY>

<canvas id='less1' width='500' height='500'>

<script>

var canvas=document.getElementById('less1');

var ctx=canvas.getContext('2d');

for(j=0;j<=10;j++)

{

var ang=j*10;

var delta=0;

var y=0;

for(i=0; i<100;i++)

{

ctx.fillRect(10+i,10+y,1,1);

ctx.fillRect(10+y,10+i,1,1);

delta +=ang;

if (delta>=50)

{

delta-=100;

y++;

}

}

}

</script>

</canvas>

</BODY>

</HTML>
